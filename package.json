{
  "name": "finalyear-project",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "finalyear-frontend",
    "finalyear-backend"
  ],
  "scripts": {
    "start": "cd finalyear-backend && npm start",
    "dev": "cd finalyear-frontend && npm run dev",
    "build": "cd finalyear-frontend && npm run build",
    "install:all": "npm install && cd finalyear-frontend && npm install && cd ../finalyear-backend && npm install"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.27.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "react-icons": "^5.5.0"
  }
}
